{
	"name": "TILApp",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"test": "jest"
	},
	"dependencies": {
		"eslint-plugin-react": "^6.10.3",
		"immutable": "^3.8.1",
		"lodash": "^4.17.4",
		"native-base": "^2.1.1",
		"react": "16.0.0-alpha.6",
		"react-native": "0.43.2",
		"react-navigation": "git+https://github.com/react-community/react-navigation.git",
		"react-redux": "^5.0.3",
		"redux": "^3.6.0",
		"redux-logger": "^3.0.1",
		"redux-persist": "^4.6.0",
		"redux-persist-immutable": "^4.2.0",
		"redux-persist-transform-immutable": "^4.2.0",
		"redux-thunk": "^2.2.0"
	},
	"devDependencies": {
		"babel-eslint": "^7.2.1",
		"babel-jest": "19.0.0",
		"babel-preset-react-native": "1.9.1",
		"eslint-config-xo-react": "^0.11.1",
		"eslint-plugin-flowtype": "^2.30.4",
		"flow-bin": "0.40.0",
		"jest": "19.0.2",
		"react-test-renderer": "16.0.0-alpha.6",
		"xo": "^0.18.1"
	},
	"jest": {
		"preset": "react-native"
	},
	"xo": {
		"parser": "babel-eslint",
		"extends": [
			"xo-react"
		],
		"ignores": [
			"flow-typed/**/*.js"
		],
		"globals": [
			"__DEV__"
		],
		"envs": [
			"node",
			"browser"
		],
		"space": 4,
		"semicolon": false,
		"plugins": [
			"flowtype"
		],
		"rules": {
			"react/forbid-component-props": [
				"off",
				{
					"forbid": []
				}
			],
			"prefer-const": [
				0
			],
			"no-multiple-empty-lines": [
				0
			],
			"import/prefer-default-export": [
				0
			],
			"react/jsx-tag-spacing": [
				0
			],
			"react/jsx-space-before-closing": [
				0
			],
			"react/jsx-closing-bracket-location": [
				1,
				"tag-aligned"
			]
		}
	}
}
